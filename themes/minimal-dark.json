  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6A6A6A"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol",
        "string.template"
      ],
      "settings": {
        "foreground": "#A7C7E7"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#FFF1A8"
      }
    },
    {
      "scope": [
        "keyword",
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#D6B3FF"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#F6A5C0"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "variable.function"
      ],
      "settings": {
        "foreground": "#B7E4C7"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "meta.property-name",
        "entity.other.attribute-name",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#D6B3FF"
      }
    },
    {
      "scope": [
